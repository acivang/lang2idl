{
    "name": "lang2idl",
    "version": "0.0.6",
    "description": "programming language interface to idl.and idl to programing language.",
    "scripts": {
        "build": "tsc",
        "groovy2idl": "tsc & node ./dist/index.js groovy2idl ",
        "cs2idl": "tsc & node ./dist/index.js cs2idl ",
        "idl2ts": "tsc & node ./dist/index.js idl2ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/acivang/lang2idl.git"
    },
    "keywords": [
        "idl",
        "groovy",
        "typescript",
        "c#",
        "json"
    ],
    "author": "aci",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/acivang/lang2idl/issues"
    },
    "homepage": "https://github.com/acivang/lang2idl#readme",
    "dependencies": {
        "optimist": "^0.6.1",
        "typescript": "^1.8.10"
    },
    "main": "./dist/index.js",
    "bin": {
        "groovy2idl": "groovy2idl.js",
        "cs2idl": "cs2idl.js",
        "idl2ts": "idl2ts.js"
    },
    "preferGlobal": true
}